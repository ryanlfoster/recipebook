//general
body {
  background: #eeeeee;
  min-width: 320px;
}

body .content * {
  font-size: 16px;
}

body.big1 .content * {
  font-size: 105% !important;
}

body.big2 .content * {
  font-size: 110% !important;
}

body.big3 .content * {
  font-size: 115% !important;
}

fieldset {
  position: relative;
}

fieldset legend {
  background: none;
}

.sub-nav {
  text-align: right;
}

.sub-nav dt, .sub-nav dd {
  float: none;
  display: block;
  margin: 0;
  line-height: 4em;
}

.loading {
  background: url("images/loading.gif") 0 0 no-repeat transparent;
  width: 200px;
  height: 200px;
  margin: 0 auto;
}

.loading-small {
  background: url("images/loading-small.gif") 0 0 no-repeat transparent;
  width: 16px;
  height: 16px;
  float: right;
  position: relative;
  top: -33px;
  right: 6px;
}

.recipeEditWrapper fieldset legend.wide {
  width: 30%;
}

fieldset .additionalItems label {
  margin: 10px 0;
}

fieldset #instructions {
  margin: 10px 0;
}

fieldset .fieldsetButtonsTop {
  position: absolute;
  top: -7px;
  right: 1.25em;
}

fieldset .fieldsetButtonsBottom {
  position: absolute;
  bottom: -30px;
  right: 1.25em;
}

fieldset .fieldsetEditButtonsBottom .fieldsetButtonSave {
  top: -46px;
  position: relative;
}

.cke_top {
  display: none !important;
}

.searchWrapper .columns {
  padding-bottom: 10px;
}

.searchWrapper .searchResult {
  height: 20em;
  overflow: hidden;
}

.searchWrapper .searchResult .searchResultTitle {
  float: left;
  width: 75%;
}

.searchWrapper .searchResult .rating {
  float: right;
}

.searchWrapper .searchResult .searchResultText {
  overflow: hidden;
  line-height: 1.25em;
}

.searchWrapper .searchResult img {
  max-height: 14em;
  cursor: pointer;
}

.clearSearch {
  float: right;
}

.clearSearch .close {
  top: 0.300em;
  right: 0.250em;
}

.clearSearchQuery {
  background: url("data:image/gif;base64,R0lGODlhGAAeAKIHAOvr69DQ0N3d3b+/v8DAwP////Pz8////yH5BAEAAAcALAAAAAAYAB4AAAN3eLrc/jDKSau9OGtnhBcGZgREWQ5BSAHDeQ4tMBkvTNitCgVwANwxHipCixVYBMDRpmsIbiUlQGojCCDPqvL4smJNsC2rVbo+suEjtTUwc8jSW7zUbJBQyLAwIDGw/0AwdQ5jbFByFSNdBCkiHyAbkZKTlJWWlAkAOw==");
  height: 30px;
  position: absolute;
  right: 17px;
  top: 50px;
  width: 24px;
  z-index: 7;
  cursor: pointer;
}

ul.ui-autocomplete {
  max-width: 17em !important;
}

ul.ui-autocomplete a.ui-state-focus {
  cursor: pointer;
}

.addToMealPlan {
  font-size: .5em;
  cursor: pointer;
}

/* @group stars */
.stars {
  background: url('images/stars.png') no-repeat scroll -65px 0 transparent;
  display: inline-block;
  margin: 0;
  overflow: hidden;
  padding: 0;
  position: relative;
  vertical-align: middle;
  height: 13px;
  width: 65px;
  cursor: pointer;
}

.stars0 {
  background-position: -65px 0 !important;
}

.stars1 {
  background-position: -52px 0 !important;
}

.stars2 {
  background-position: -39px 0 !important;
}

.stars3 {
  background-position: -26px 0 !important;
}

.stars4 {
  background-position: -13px 0 !important;
}

.stars5 {
  background-position: 0 0 !important;
}

.starsArea {
  width: 13px;
}

/* @end stars */

/* @group mealplan */
.mealplan {
  position: relative;

  -webkit-transition: opacity 0.15s linear;
  -moz-transition: opacity 0.15s linear;
  -o-transition: opacity 0.15s linear;
  transition: opacity 0.15s linear;
}

.mealplan.callout {
  border-color: #DADFE6;
  background: #DADFE6;
}

.mealplan.callout:hover {
  border-color: #575B5F;
}

.mealplan .delete {
  position: absolute;
  right: 4px;
  top: 4px;
}

.mealplan .move {
  position: absolute;
  left: 4px;
  top: 4px;
}

.mealplan .content img {
  max-height: 8em;
}

/* @end mealplan */

@media only screen and (min-width: 635px) {
  .cke_top {
    display: block !important;
  }
}
